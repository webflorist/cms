{
    "name": "webflorist/cms",
    "description": "Webflorist CMS for Laravel (v5.5+ and v6)",
    "authors": [
        {
            "name": "Gerald Buttinger",
            "email": "gerald@code.florist"
        }
    ],
    "require": {
    	"laravel/framework": ">=5.5 <7.0.0",
        "webflorist/htmlfactory": "1.1.*",
        "webflorist/routetree": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5",
        "gajus/dindent": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Webflorist\\Cms\\": "src/",
	        "CmsTests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webflorist\\Cms\\CmsServiceProvider"
            ],
            "aliases": {
                "Cms": "Webflorist\\Cms\\CmsFacade"
            }
        }
    }
}
