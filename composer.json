{
    "name": "webflorist/cms",
    "description": "Webflorist CMS for Laravel 5.5 upwards",
    "authors": [
        {
            "name": "Gerald Buttinger",
            "email": "gerald@code.florist"
        }
    ],
    "require": {
		"laravel/framework": "~5.5",
        "webflorist/htmlfactory": "dev-develop",
        "webflorist/iconfactory": "dev-develop",
        "webflorist/routetree": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5",
        "gajus/dindent": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Webflorist\\Cms\\": "src/",
	        "CmsTests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/webflorist/iconfactory",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/webflorist/routetree",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/webflorist/htmlfactory",
            "no-api": true
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Webflorist\\Cms\\CmsServiceProvider"
            ],
            "aliases": {
                "Cms": "Webflorist\\Cms\\CmsFacade"
            }
        }
    }
}
