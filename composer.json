{
    "name": "webflorist/cms",
    "description": "Webflorist CMS for Laravel 5.5 upwards",
    "authors": [
        {
            "name": "Gerald Buttinger",
            "email": "gerald@code.florist"
        }
    ],
    "require": {
		"laravel/framework": "~5.5"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5"
    },
    "autoload": {
        "psr-4": {
            "Webflorist\\Cms\\": "src/",
	        "CmsTests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webflorist\\Cms\\CmsServiceProvider"
            ]
        }
    }
}
